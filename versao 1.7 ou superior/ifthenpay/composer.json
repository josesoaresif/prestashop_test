{
    "name": "ifthenpay/ifthenpay",
    "description": "Ifthenpay gateway for prestashop",
    "type": "prestashop-module",
    "require": {
        "php": ">=7.0.0"
    },
    "autoload": {
        "psr-4": {
            "PrestaShop\\Module\\Ifthenpay\\": "classes/",
            "Test\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "prepend-autoloader": false
    },
    "authors": [
        {
            "name": "Ifthenpay",
            "email": "ifthenpay@ifthenpay.com"
        }
    ],
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "codeception/codeception": "^4.1",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-asserts": "^1.0.0"
    }
}